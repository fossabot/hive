package provider

import (
	rpc{{.PkgNameCamel}} "{{.Repo}}/go-pkg/rpc-{{.PkgName}}"
    "google.golang.org/grpc"
)

func ClientProvider (server string, options ...grpc.DialOption) (rpc{{.PkgNameCamel}}.{{.PkgNameCamel}}Client, error) {
	conn, err := grpc.Dial(server + ":{{.Port}}", options...)
	if err != nil {
		panic("Cannot dial")
	}

	return rpc{{.PkgNameCamel}}.New{{.PkgNameCamel}}Client(conn), nil
}
